{
  "name": "voice-scam-shield-mvp",
  "version": "1.0.0",
  "description": "Voice Scam Shield - Real-time multilingual AI scam detection for phone calls via Twilio integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:deepfake": "npm run test -- deepfake",
    "test:scam-analysis": "npm run test -- scam-analysis",
    "test:twilio-integration": "npm run test -- twilio",
    "demo:scam-call": "node demo/scam-call-demo.js",
    "demo:legitimate-call": "node demo/legitimate-call-demo.js",
    "build": "npm run build:client && npm run build:docs",
    "build:client": "webpack --mode production",
    "build:docs": "jsdoc -c jsdoc.conf.json",
    "deploy": "npm run build && npm run deploy:server",
    "deploy:server": "echo 'Deploy to your cloud platform'",
    "setup": "node scripts/setup.js",
    "health-check": "node scripts/health-check.js",
    "clean": "rimraf dist/ docs/ temp/ *.log"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.0",
    "groq-sdk": "^0.3.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.6.7",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.1",
    "twilio": "^4.23.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "keywords": [
    "voice-scam-detection",
    "twilio-integration",
    "real-time-ai",
    "deepfake-detection",
    "multilingual-support",
    "fraud-prevention",
    "cybersecurity",
    "mit-hackathon"
  ],
  "author": "Voice Scam Shield Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/voice-scam-shield-mvp.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/voice-scam-shield-mvp/issues"
  },
  "homepage": "https://github.com/your-org/voice-scam-shield-mvp#readme",
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true,
      "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    }
  }
}
